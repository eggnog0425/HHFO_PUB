<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="CommonSetting.xml" />
    <None Remove="DispMessage.xml" />
    <None Remove="ErrorMessage.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="CommonSetting.xml" />
    <EmbeddedResource Include="ErrorMessage.xml" />
